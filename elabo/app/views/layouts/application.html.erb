  <body>

    <header class="header">
  <!-- Header Inner -->
  <div class="header-inner">
    <div class="container-fluid pt-2">
      <div class="inner">
        <div class="row">
          <div class="col-lg-3 col-md-3 col-12">
            <div class="logo">
              <%= link_to image_tag('frontend/img/logo.png', alt: 'Logo'), root_path %>
            </div>
          </div>
          <div class="col-lg-9 col-md-9 col-12">
            <div class="container-fluid">
              <div class="main-menu">
                <nav class="navigation nav">
                  <ul class="nav menu">
                    <% if user_signed_in? %>
                      <!-- Options de menu pour un laborantin -->
                      <% if current_user.laborantin? %>
                        <li class="nav-item me-3 <%= 'active' if current_page?(manage_laboratories_path) %>">
                          <%= link_to 'Gérer mes laboratoires', manage_laboratories_path %>
                        </li>
                      <!-- Options de menu pour un patient -->
                      <% elsif current_user.patient? %>
                      <li class="nav-item me-3 nav-link <%= 'active' if current_page?(root_path) %>"><%= link_to 'Accueil', root_path %></li>
                      <li class="nav-item me-3 nav-link <%= 'active' if current_page?(laboratories_path) %>"><%= link_to 'Laboratoires', laboratories_path %></li>
                      <li class="nav-item me-3 nav-link <%= 'active' if current_page?(contact_path) %>"><%= link_to 'Contacter Nous', contact_path %></li>
                        <li class="nav-item me-3 <%= 'active' if current_page?(patient_dashboard_path) %>">
                          <%= link_to 'Mes rendez-vous', patient_dashboard_path %>
                        </li>
                        <li class="nav-item me-3 <%= 'active' if current_page?(laboratories_path) %>">
                          <%= link_to 'Rechercher des examens', laboratories_path %>
                        </li>
                      <% end %>

                      <!-- Profil utilisateur et déconnexion -->
                      <li class="nav-item me-3"><%= link_to current_user.name, profile_path(current_user) %></li>
                      <li class="nav-item me-3"><%= link_to 'Se déconnecter', destroy_user_session_path, class: 'btn-md' %></li>

                    <% else %>
                      <!-- Si l'utilisateur n'est pas connecté -->
                      <li class="nav-item me-3"><%= link_to 'Se connecter', new_user_session_path, class: 'btn' %></li>
                      <li class="nav-item"><%= link_to 'S\'inscrire', new_user_registration_path, class: 'btn' %></li>
                    <% end %>
                  </ul>
                </nav>

              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
  <!--/ End Header Inner -->
</header>



